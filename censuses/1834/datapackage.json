      "schema": {
        "fields": [
          {
            "name": "ft",
            "type": "integer",
            "description": "The year of the census."
          },
          {
            "name": "sogn",
            "type": "string",
            "description": "The parish in which the location is situated."
          },
          {
            "name": "amt",
            "type": "string",
            "description": "The county of which the parish is part."
          },
          {
            "name": "id",
            "type": "integer",
            "description": "The number of the person. Unique within the census_year and parish."
          },
          {
            "name": "loknr",
            "type": "integer",
            "description": "The number of the building."
          },
          {
            "name": "lokalitet",
            "type": "string",
            "description": "Specific location of the building."
          },
          {
            "name": "husnr",
            "type": "string",
            "description": "Indicating the number or interval of numbers of the house"
          },
          {
            "name": "bygning",
            "type": "string",
            "description": "Describes or numbers the building."
          },
          {
            "name": "famnr",
            "type": "integer",
            "description": "The number of the family. Unique within the location (lokalitet)."
          },
          {
            "name": "fnavn",
            "type": "string",
            "description": "Firstname(s) of the person."
          },
          {
            "name": "enavn",
            "type": "string",
            "description": "Lastname(s) of the person."
          },
          {
            "name": "koen",
            "type": "string",
            "description": "Mostly male (M) or female (K). Other encoded values and blanks occur."
          },
          {
            "name": "famstand",
            "type": "string",
            "description": "Personal position in the household."
          },
          {
            "name": "alder",
            "type": "integer",
            "description": "Age of the person."
          },
          {
            "name": "civilstand",
            "type": "string",
            "description": "Marital status. Usually one of the following values: gift (married), fraskilt/skilt (divorced), separeret (divorced), enke (widow), enkemand (widower), ugift (not married), forlovet (engaged)."
          },
          {
            "name": "erhverv",
            "type": "string",
            "description": "Occupation(s) of the person."
          },
          {
            "name": "notater",
            "type": "string",
            "description": "Notes from transcribers."
          }
        ]
